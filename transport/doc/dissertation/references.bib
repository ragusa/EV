@ARTICLE{Seufert2002,
  author = {Seufert, Sabine and Lechner, Ulrike and Stanoevska, Katarina},
  title = {A Reference Model for Online Learning Communities},
  year = {2002},
  volume = {1},
  pages = {43-54--},
  __markedentry = {[wendy]},
  abstract = {Online learning communities are introduced as a comprehensive model
	for technology-enabled learning. We give an analysis of goals in
	education and the requirements to community platforms. The main contribution
