@ARTICLE{Hawkins2010,
  author = {Hawkins, Abigail and Barbour, Michael K.},
  title = {U.S. Virtual School Trial Period and Course Completion Policy Study},
  year = {2010},
  volume = {24},
  pages = {5-20--},
  __markedentry = {[wendy]},
  abstract = {Variation in policies virtual schools use to calculate course completion
	and retention rates impacts the comparability of these quality metrics.
	This study surveyed 159 U.S. virtual schools examining the variability
	in trial period and course completion policiesâ€”two policies that
	affect course completion rates. Of the 86 respondents, almost 70%
	had trial periods that varied from 1 day to 185 days. Course completion
	definitions varied from remaining in the course irrespective of the
	final grade to receiving an A- in the course. Such wide variation
