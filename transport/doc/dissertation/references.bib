@ARTICLE{Channon2010,
  author = {Channon, Shelley and Fitzpatrick, Sian and Drury, Helena and Taylor,
	Isabelle and Lagnado, David},
  title = {Punishment and Sympathy Judgments: Is the Quality of Mercy Strained
	in Asperger's Syndrome?},
  year = {2010},
  volume = {40},
  pages = {1219-1226--},
  __markedentry = {[wendy]},
  abstract = {This study examined reasoning about wrongdoing in people with Aspergers
	syndrome (AS) and matched healthy controls in relation to car accident
	scenarios. The two groups made similar judgments with respect to
	degree of driver negligence for both fines imposed and sympathy ratings.
	They also made similar judgments of fines in relation to the type
	of justification given for the drivers actions. However, the AS group
	differentiated more in sympathy judgments relating to good and poor
	justifications. The AS group thus appeared to show preserved judgment
	with respect to compensation and sympathy for the victim and fines
	for the driver, but expressed less sympathy towards drivers with
	poor justifications for their actions.},
