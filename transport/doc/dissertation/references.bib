@ARTICLE{Gold2010,
  author = {Gold, Rinat and Faust, Miriam},
  title = {Right Hemisphere Dysfunction and Metaphor Comprehension in Young
	Adults with Asperger Syndrome},
  year = {2010},
  volume = {40},
  pages = {800-811--},
  __markedentry = {[wendy]},
  abstract = {This study examined whether the known difficulties in metaphor comprehension
	exhibited by persons with Asperger syndrome (AS) can be explained
	by a dysfunctional right hemisphere (RH). Using the divided visual
	field paradigm, 27 AS participants and 36 matched controls were presented
	with word pairs of four types (literal, conventional metaphors, novel
	metaphors, and unrelated word pairs), and were asked to perform a
	semantic judgment task. The main hypothesis was that whereas the
	control group participants will show RH superiority for novel metaphor
	processing, no RH superiority will be found in the AS group. Results
	indeed indicate much less RH contribution to novel metaphor comprehension
	in AS, and are discussed in light of linguistic models and the neurobiology
	of autism.},
